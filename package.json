{
  "name": "monothor",
  "version": "1.0.0",
  "description": "Extra powerfull monitor package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:juazugas/MonoThor.git",
  "scripts": {
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "postinstall": "webpack -p",
    "start": "node server.js",
    "lint": "eslint src",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "cover": "babel-node ./node_modules/isparta/bin/isparta cover --report text --report html _mocha -- --require ./test/test_helper.js ./test/**/*_test.js -R spec "
  },
  "author": "Juan Zuriaga",
  "license": "MIT License",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.2.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "chai-string": "^1.3.0",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.8.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "sinon": "^2.1.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "babel-preset-stage-1": "^6.1.18",
    "lodash": "^3.10.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "redux": "^3.0.4"
  }
}
